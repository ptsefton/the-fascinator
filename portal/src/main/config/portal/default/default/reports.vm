#set($pageTitle = "Harvest Log")
<div class="grid_6">
  #set($menuTitle = "Harvest logs")
  <div class="block">
    #if(!$self.latest)
      &nbsp;
    #else
      <ol>
      #foreach($item in $self.latest)
        #set($harvestId = $item.getFirst('harvestId'))
        #set($repoType = $item.getFirst('repoType'))
        #set($repoName = $item.getFirst('repoName'))
        <li class="">
          <a href="$portalPath/harvestreport/$harvestId">$repoType $repoName - $harvestId</a>
        </li>
      #end
      </ol>
    #end
  </div>
</div>